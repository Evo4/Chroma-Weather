# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'Chroma Weather' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Chroma Weather
    pod 'RxSwift', '~> 5'
    pod 'RxCocoa', '~> 5'
    pod 'RxGoogleMaps'
    pod 'GooglePlaces'
    pod 'GoogleSignIn'
    pod 'FBSDKLoginKit'
    pod 'RxCoreLocation'
    pod 'Alamofire'
    pod 'AFDateHelper'
    pod 'RxRealm'
    
    target 'WeatherAPIKit' do
        inherit! :search_paths
    end
    
    target 'Chroma Weather Widget' do
        inherit! :search_paths
    end
    
end
